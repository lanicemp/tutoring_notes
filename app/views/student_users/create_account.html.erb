<div class="container">
  <header><img height="164" width="290" class="media-element file-default panopoly-image-original" src="https://castlemont.campbellusd.org/system/files/styles/panopoly_image_original/private/tutoring-gig.gif?itok=MRhYo1ZS" alt=""></header>
  <div class="col-md-8 col-md-offset-2">

<h2> Create Account </h2>

<%= "Must include email and password" if @student && @student.errors.any %>
<form action="create_account" method="POST"> 
<br>
<p>
    <label for="name">Name:</label>
    <input type="text" name="name" id="name"/>
    <br>
    <br>
    <label for= "age">Age:</label>
    <input type="text" name="age" id="age" />
    <br>
    <br>
    <label for= "grade">Grade:</label>
    <input type="text" name="grade" id="grade" />
    <br>
    <br>
    <label for= "email">Email:</label>
    <input type="text" name="email" id="email" />
    <br>
    <br>
    <label for= "username">Username:</label>
    <input type="text" name="username" id="username" />
    <br>
    <br>
    <label for= "password">Pasword:</label>
    <input type="password" name="password" id="password" />
    <br>
    <br>
    <label for= "password_confirmation">Pasword Confirmation:</label>
    <input type="password" name="password_confirmation" id="password_confirmation" />
</p>

<input type="submit" value="create account" /> 

</form>
